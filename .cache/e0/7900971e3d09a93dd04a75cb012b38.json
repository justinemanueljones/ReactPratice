n":8}},{"generated":{"line":108,"column":92},"source":"components/Programmers.js","original":{"line":79,"column":12}},{"generated":{"line":109,"column":0},"source":"components/Programmers.js","original":{"line":79,"column":17}},{"generated":{"line":109,"column":4},"source":"components/Programmers.js","original":{"line":79,"column":17}},{"generated":{"line":109,"column":9},"source":"components/Programmers.js","original":{"line":79,"column":22}},{"name":"style","generated":{"line":109,"column":11},"source":"components/Programmers.js","original":{"line":79,"column":24}},{"generated":{"line":110,"column":0},"source":"components/Programmers.js","original":{"line":79,"column":12}},{"generated":{"line":110,"column":34},"source":"components/Programmers.js","original":{"line":59,"column":4}},{"generated":{"line":110,"column":35},"source":"components/Programmers.js","original":{"line":58,"column":2}},{"generated":{"line":111,"column":0},"source":"components/Programmers.js","original":{"line":83,"column":1}}],"sources":{"components/Programmers.js":"/*\nPROGRAMMERS Instructions\n\nWatch this short video:\nhttps://tk-assets.lambdaschool.com/1ea6e6a2-2ef1-45de-bcab-b99a8f775870_programmers.gif\n\nThis component keeps track of a list of pioneers in the field of programming on the one hand,\nand the id of the currently featured programmer on the other. That's two slices of state!\nWe can only feature one awesome programmer at a time.\n\nFind comments below to help you along.\n*/\n\nimport React, { useState } from 'react';\n\n// Use this variable ONLY to initialize a slice of state!\n// There is something in the JSX right now breaking this rule...\nconst listOfAwesome = [\n  { id: '1', name: 'Ada Lovelace' },\n  { id: '2', name: 'Grace Hopper' },\n  { id: '3', name: 'Evelyn Boyd Granville' },\n  { id: '4', name: 'Mary Kenneth Keller' },\n  { id: '5', name: 'Frances Allen' },\n  { id: '6', name: 'Carol Shaw' },\n];\n\nexport default function Programmers() {\n  // We'll have to use the state hook twice, as we need two slices of state.\n  // The programmers list on the one hand, and the id of the featured programmer on the other.\n  const [programmers, setProgrammers] = useState(listOfAwesome);\n\n  console.log(\"programmers\", programmers);\n  const [programmerId, setProgrammerId] = useState(null);\n\n  const getNameOfFeatured = () => {\n    // Leave this for last!\n    // This is NOT an event handler but a helper function. See its usage inside the JSX.\n    // It's going to utilize both slices of state to return the _name_ of the featured dev.\n    // The beauty of closures is that we can \"see\" both slices of state from this region\n    // of the program, without needing to inject the information through arguments.\n\n    if(programmerId === null) {\n      return null\n    } else {\n      const featuredProgrammer = programmers.find(programmer => {\n        return programmerId === programmer.id\n      })\n      return featuredProgrammer.name\n    };\n  };\n\n  const style = {\n    fontSize: '1.5em',\n    marginTop: '0.5em',\n    color: programmerId !== null ? 'green' : 'royalblue', // ðŸ¤”\n  };\n\n  return (\n    <div className='widget-programmers container'>\n      <h2>Programmers</h2>\n      <div className='programmers'>\n        {\n          /* Nasty bug! We should map over a slice of state, instead of 'listOfAwesome'.\n          We might think: \"it works, though!\" But if the list of programmers is not state,\n          we could never add or edit programmers in the future. The list would be a static thing.\" */\n          programmers.map(dev =>\n            <div key={dev.id}>\n              {dev.name} <button onClick={() => { setProgrammerId(dev.id) }}>Feature</button>\n            </div>\n          )\n        }\n      </div>\n      {\n        // Ternaries are fantastic to render \"one thing or the other\" depending on the \"truthiness\" of something.\n        // Pseudo-code: if the currently featured id is truthy render div 1, otherwise render div 2.\n        // Replace the hard-coded false with the correct variable.\n        programmerId !== null\n          ? <div style={style}>ðŸŽ‰ Let&apos;s celebrate {getNameOfFeatured()}! ðŸ¥³</div>\n          : <div style={style}>Pick an awesome programmer</div>\n      }\n    </div>\n  );\n}\n"},"lineCount":null}},"error":null,"hash":"43349f65731268f47fa3f0d4ef7c7094","cacheData":{"env":{}}}